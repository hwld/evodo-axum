{
    "eslint.workingDirectories": [
        {
            "pattern": "./client/"
        }
    ],
    "rust-analyzer.check.command": "clippy",
    "rust-analyzer.showUnlinkedFileNotification": false,
    "editor.wordWrap": "on",
    "tailwindCSS.classAttributes": [
        ".*Class",
        "class",
        "className",
        "classNames",
        "ngClass"
    ],
}